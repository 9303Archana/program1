    <title>Lab program 2a</title>
</head>
<script type="text/javascript">
    function fib()
    {
       var fib1=0;
       var fib2=1;
        var fib=0;
        var i;
        var n=prompt("Enter Number");
        document.write("Fibonacci Number");
        for(i=0;i<=n;i++)
        {
            document.write("<br>"+fib1);
            fib=fib1+fib2;
            fib1=fib2;
            fib2=fib;
        }
    }

</script>
<body onload="fib()">
</body>

</html>
